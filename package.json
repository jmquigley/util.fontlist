{
  "name": "util.fontlist",
  "version": "0.0.6",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Detects the fonts that are available to the browser on the current system",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.fontlist"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.fontlist/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "devDependencies": {
    "@types/node": "^8.0.54",
    "debug": "^3.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "pkgwrap": {
    "cleanup": [
      "index.d.ts",
      "index.js"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "lint": "pkgwrap --lint",
    "preinstall": "yarn global add util.pkgwrap"
  }
}
